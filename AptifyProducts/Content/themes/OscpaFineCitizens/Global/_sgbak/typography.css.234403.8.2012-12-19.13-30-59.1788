h1 {
	font-family: "league-gothic", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	font-size: 24px;
	line-height: 1.2em;
	color: #74954d;
}

h2, a.additional, .popularTitle {
	font: 35px/1.2em "league-gothic", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-transform:uppercase;
	color: #857e74;
	text-shadow: 0 2px 1px #ffffff;
	display: inline-block;
	.display: inline;
	.zoom: 1;
	vertical-align: top;
	padding-right: 1em;
}

h2 a{
	background: url(images/x/more_info_arrow.png) no-repeat center right;
	display: inline-block;
	.display: inline;
	.zoom: 1;
	vertical-align: top;
	padding-right: 1em;
}
.popularArticles{
	margin: 10px 10px 0 0;
	padding:2em;
	background:#edebe9;
	border-radius: 5px;
	
}

.popularTitle{
	background: none;
/* 	margin-left: 35px; */
	display: block;
	margin-top: 2em;
	
}
.popularArticle h2.sfpostTitle{
	font: 24px/1.2em "league-gothic", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-transform:uppercase;
	color: #4483b9;
	margin-top: 15px;
	background: none;
	display: block;
	padding-bottom: .5em;
}

article.stories .popularArticle a.full-story{
	float: none;
	margin-left: 25px;
	text-decoration: none;
	font-size: 1.2em;
}
h3, h5 {
	font: 24px/1.2em "league-gothic", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-transform:uppercase;
	color: #4483b9;
	margin-top: 15px;
}
h4 {
	font: 30px/1.2em "league-gothic", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-transform:none;
	color: #75954c;
	margin-top: 15px;
}
h5{
	margin-top: 15px;
}
h3.related{
	color: #4483b9;
}
.stories p {
	color: #857e75;
	font-size: 14px;
	line-height: 1.4em;
	padding-top: 10px;
}
.stories ul{
	color: #857e75;
	font-size: 14px;
	line-height: 1.4em;
	padding-top: 10px;
	list-style: disc;
	list-style-position: outside;
	margin: 0;
	margin-left: 15px;
	margin-bottom: 15px;
}
a.additional {
	font-size: 24px;
	line-height: 2.6em;
	cursor: pointer;
}

span.caption {
	color: #4c4645;
	font-size: 12px;
	font-style: italic;
	line-height: 1.6em;
	display: inline-block;
}

.article_post{
	padding: 0 6% 0 0;
	font-size: 13px;
	line-height: 1.6em;
	color: #333;
}
.article_post .sfpostTitle{
	padding: 0 0 .5em;
	text-transform: uppercase;
}
.article_post .sfpostAuthorAndDate{
	padding-bottom: .5em;
}
.article_post .sfpostLinksWrp{
	text-align: right;
}
.article_post .sfpostLinksWrp a{
	font-size: 1em;
	color: #888;
}
.relatedArticles h1{
	font-size: 2em;
}

article.stories .relatedArticles div.snippet{
	margin-top: 10px;
}
article.stories .relatedArticles div.snippet-text{
	font-size: 1em!;
}
article.stories .similar .snippet a{
	padding: 0;
	font-size: 1.1em!important;
}
article.stories .similar .snippet{
	font-size: 10px!important;
}
article.stories .similar .snippet-text{
	line-height: 1.4em!important;
}
article.stories .similar a.full-story{
	padding-right: 1em;
	margin-top: 1em;
	
	font-size: 1.4em!important;
}
article.stories .similar .popularTitle{
	margin-top:1.2em!important;
}

.stories table{
	border:none;
	padding: 0;
	border: 1px solid #ccc; 
	border-spacing: 0;
	background: #fff;
	border-collapse:collapse;
}

.stories table th{
	background-color: #4483b9;
	color:#fff;
	font-weight: bold;
	font-size: 12px;
	padding: 10px;
	margin: 0;
}

.stories table tr td{
	padding: 10px;
	font: normal 11px 'Lato', Arial, serif;
	text-align: left;
	border-left:1px solid #ccc;
	color: #777;
}

.stories table tr td:first-child{
	border-left:none;
}
table tr:nth-child(2n) {
	background: #f6f5f3;
}
label{
	padding-right: 10px;
	padding-left: 10px;
	font-size: 14px;
}
input[type="password"],input[type="search"],input[type="text"], select{
	border: 1px solid #333;
	border-radius: 5px;
	padding: 6px 10px;
	color: #777;
	font-size: 14px;
	background: #fff;
	margin-bottom: 10px;
	-moz-appearance: none;
	-webkit-appearance: none;
}
input[type=submit]{
	background: -moz-linear-gradient(90deg, #789950 0%, #b4d590 100%) #99ba72;
	background: -webkit-gradient(linear, left bottom, left top, from(#789950), to(#b4d590)) #99ba72;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#b4d590', endColorstr='#789950');
	color: #fff;
	font-size: 14px;
	border: none;
	border-radius: .5em;
	behavior: url(PIE.htc);
	padding: 6px 10px;
}
.sfsearchBox input[type=submit].sfsearchSubmit{
	position: absolute;
	right: 4px;
	top:4px;
    padding :0;
	text-indent: -800em;
	width: 2em;
	background: #fff url(images/x/icon-search.png) no-repeat center center;
    filter:none;
	height: 25px;
}


a.RadButton{
	padding-right:0 !important;
	background:none !important;
}

.RadButton input[type=submit]{
	padding: 3px 5px;
	font-size: .9em;
	background: -moz-linear-gradient(90deg, #789950 0%, #b4d590 100%) #99ba72 !important;
	background: -webkit-gradient(linear, left bottom, left top, from(#789950), to(#b4d590)) #99ba72 !important;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#b4d590', endColorstr='#789950') !important;
}

select{
	background: #fff url(images/down.png) no-repeat right center;
	padding-right: 32px;
}
.accordion{
	margin-bottom: 4px; 
	margin-top: 20px;
	position: relative;
	left: -3.3%;
}
.accordion .title, .ui-accordion .ui-accordion-header{
	background: #857e74 url('images/accord-closed.png') no-repeat right center;
	color: white;
	padding: 15px 10px;
	font-weight: bold;
	font-size: 12px;
	font-family: Arial;
	border-radius: 5px;
	cursor: pointer;
}
.accordion .content{
	display: none;
	background: #edecea;
	border:1px solid #ccc;
	border-radius: 5px;
	border-top-left-radius:0;
	border-top-right-radius:0;
	margin: 0px 4px;
	font-size: 12px;
	padding: .8em;
}
.accordion.open .title, .ui-accordion .ui-accordion-header.ui-state-active{
	background: #4483b9 url('images/accord-open.png') no-repeat right center;
}
.accordion.open .content, .ui-accordion .ui-accordion-content{
	display: block;
}

article.stories .RadRating_Default a{
	padding-right:0
}

.ui-datepicker{
	background:#fff;
	font-family: "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	font-size:1.2em;
	border:2px solid #888;
	box-shadow:0px 5px 5px #ccc;
}

.ui-datepicker .ui-datepicker-calendar td{
	padding:5px 8px;
}
.ui-datepicker .ui-datepicker-calendar td a{
	color:#333;
	text-decoration:none;
}
.ui-datepicker .ui-datepicker-calendar td:hover{
	background:#4483b9;
}
.ui-datepicker-header{
	background: #4483b9;
	padding:5px;
	text-align:center;
	position:relative;
	color:white;
}
.ui-datepicker-calendar th{
	padding:5px 8px;
	background:#333;
	color:#ccc;
}
.ui-datepicker-header .ui-datepicker-prev{
	position:absolute;
	left:10px;
	top:5px;
}
.ui-datepicker-header .ui-datepicker-next{
	position:absolute;
	right:10px;
	top:5px;
}
.ui-datepicker-other-month{
	background:#888;
}

.ui-dialog{
	position:relative;
}
.ui-dialog-titlebar{
	background:#333;
	color:#ccc;
	font-family: "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	font-size:1.2em;
	padding:10px;
	position:relative;
}
.ui-dialog-titlebar-close{
	position:absolute;
	right:10px;
	top:10px;
	color:white;
}
.ui-dialog-content{
	background:#eee;
	border:2px solid #333;
	padding:10px;
	font-family: "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	font-size:1.2em;
	line-height:1.6em;
	color:#333;
}
.ui-tabs{
	
}
.ui-tabs .ui-tabs-nav{
	padding:0;
	margin:0;
}
.ui-tabs .ui-tabs-nav li{
	display:inline-block;
	.display:inline;
	.zoom:1;
	vertical-align:top;
	background: -moz-linear-gradient(90deg, #789950 0%, #b4d590 100%) #99ba72;
	background: -webkit-gradient(linear, left bottom, left top, from(#789950), to(#b4d590)) #99ba72;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#b4d590', endColorstr='#789950');
	border-radius:5px;
	border-bottom-left-radius:0;
	border-bottom-right-radius:0;
	color:#5b743c;
	font-size:.8em;
    padding-right:10px;
}
.ui-tabs .ui-tabs-nav li a{
    padding:0 10px;
}
.ui-tabs .ui-tabs-nav .ui-state-active{
	background: #b4d590;
	color:#000;
}
.ui-tabs .ui-tabs-nav li a{
	text-decoration:none;
	color:#5b743c;
	padding-right:0;
}
.ui-tabs .ui-tabs-nav .ui-state-active a{
	text-decoration:none;
	color:#000;
}
.ui-tabs .ui-tabs-nav a:hover{
	color:#000!important;
	
}
.ui-tabs-panel{
	border:2px solid #b4d590;
	padding:1em;
}
.ui-tabs-hide{
	display:none;
}


article.stories .button_general{
	background: -moz-linear-gradient(90deg, #789950 0%, #b4d590 100%) #99ba72;
	background: -webkit-gradient(linear, left bottom, left top, from(#789950), to(#b4d590)) #99ba72;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#b4d590', endColorstr='#789950');
	color: #4c4544;
	font-size: 14px;
	border-radius: .5em;
	behavior: url(PIE.htc);
	padding: .5em;
	font-weight: bold;
	display: inline-block;
	vertical-align: top;
	position: relative;
	padding-right: 3em;
	cursor: pointer;
	text-decoration: none;
	color: #fff;	
}
.ui-button, button, input[type=button]{
    background: -moz-linear-gradient(90deg, #789950 0%, #b4d590 100%) #99ba72;
	background: -webkit-gradient(linear, left bottom, left top, from(#789950), to(#b4d590)) #99ba72;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#b4d590', endColorstr='#789950');
    border-radius: .5em;
	padding: .5em;
    color: #fff;
    border:none;
}
.RadUpload .ruButton{
    padding-top:1px;
}
.button_general:hover{
	color:white!important;
}
.button_general .icon {
	height: 1.5em;
	width: 1.5em;
	display: inline-block;
	background-image: url(images/x/arrow-button-green.png);
	background-size: 100% 100%;
	-webkit-background-size: 100% 100%;
	position: absolute;
	right: .3em;
	top: .3em;
}
article.stories .button_general.no_icon{
	padding-right:.5em; 
}
.form_column_title{
	font: bold 14px/1.6em Arial, Helvetica, Geneva, sans-serif;
	color: #857e75;
}
.form_item, a.form_item {
	font: normal 1.2em/1.6em Arial, Helvetica, Geneva, sans-serif;
	color: #857e75!important;
}
a.form_item{
	text-decoration: underline;
}
a.form_item:hover{
	color: #4483b9!important;
}
.form_title{
	font: 22px/1.6em "league-gothic", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
	text-transform: uppercase;
	color: #4c4645;
}
article.stories{
	padding-left: 5%;
}
article.stories h1, article.stories h3, article.stories p{
	margin-left:0 !important;
}

article.stories .snippet h1{
	margin-left:0!important;
}
article.stories .article_post h1{
margin-left:0!important;
}
.article_post{
	margin-left:2%!important;
}
article.stories a {
	color: #000;
}
article.stories a:hover{
	color: #4483b9;
}

.stories ul{
	line-height: 2.2em!important;
}

.popularArticle a{
	padding-right: 12px;
}

article.stories h2 a,article.stories h3 a{
	color:#857E75;
	text-decoration: none;
}

article.stories a.arrow_link{
	background:url(images/x/arrow-full-story.png) no-repeat center right;
	padding-right: 15px;
	font-size: 12px;
	text-decoration: none;
	font-weight: bold;
	color:#4c4645;
	line-height: 1.6em;
}
article.stories a.arrow_link:hover{
	color:#4483b9;
}

.form_holder{
	background: #edecea;
	border: 1px solid #ccc;
	border-radius: 5px;
	padding: 20px;
	display: block;
	font-size: 1em;
	width:92%;
	box-sizing: border-box;
}
.stories .form_holder table.clear,.stories .form_holder table.clear tr,.stories .form_holder table.clear td{
	border: none;
	background: none;
	padding: 0;
	vertical-align: top;
}


article.stories .snippet h1 a{
    color:#74954d;
    font-size:24px;
    text-transform:uppercase;
    font-weight:normal;
    text-decoration:none;
}
article.stories .snippet h1 a:hover{
    color:#4483b9;

}

input[type=submit].rgPageFirst,input[type=submit].rgPageLast,input[type=submit].rgPagePrev,input[type=submit].rgPageNext{
    filter:none!important;
    background: none;
    color: black;
    font-size: 14px;
}

.rcbReadOnly input[type=text]{
    margin-bottom:0;
}

.stories ul.sfpostsList{
    list-style:none;
    margin-left:0;
}